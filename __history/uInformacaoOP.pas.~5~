unit uInformacaoOP;

interface
 uses Classes, System.SysUtils;
 type
  TinfoOP = class

    private
    FnrSiafi_42: String;

    public
    property nrSiafi_42: String read FnrSiafi_42 write FnrSiafi_42;
     function gerarTX2(): string;
  end;

implementation

{ TinfoOP }

function TinfoOP.gerarTX2: string;
var
  Lista: TStringList;
begin
  Lista := TStringList.Create;
  Lista.Add('nrSiafi_42=' + nrSiafi_42);
  Result := Lista.Text;
end;

end.
