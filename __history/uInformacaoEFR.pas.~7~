unit uInformacaoEFR;

interface

uses
  Classes, System.SysUtils;

type
  TinfoEFR = class
  private
    FcnpjEFR_45: string;
    FideEFR_44: string;
  public
    property ideEFR_44: string read FideEFR_44 write FideEFR_44;
    property cnpjEFR_45: string read FcnpjEFR_45 write FcnpjEFR_45;
    function gerarTX2(): WideString;
  end;

implementation

{ TinfoEFR }

function TinfoEFR.gerarTX2: WideString;
var
  Lista: TStringList;
begin
  Lista := TStringList.Create;
  Lista.Add('ideEFR_44=' + ideEFR_44);
  if (Trim(cnpjEFR_45) <> EmptyStr) then
    Lista.Add('cnpjEFR_45=' + cnpjEFR_45);
  Result := Lista.Text;
end;

end.

