unit uInformacaoInternacional;

interface

uses
  Classes, System.SysUtils;

type
  TinfoOrgInternacional = class
  private
    FindAcordoIsenMulta_54: string;
  public
    property indAcordoIsenMulta_54: string read FindAcordoIsenMulta_54 write FindAcordoIsenMulta_54;
    function gerarTX2(): WideString;
  end;

implementation

{ TinfoOrgInternacional }

function TinfoOrgInternacional.gerarTX2: WideString;
var
  Lista: TStringList;
begin
  Lista := TStringList.Create;
  Lista.Add('indAcordoIsenMulta_54=' + indAcordoIsenMulta_54);
  Result := Lista.Text;

end;

end.

