unit uInformacaoContato;

interface
uses System.Classes, System.SysUtils;
type
 Tcontato = class
   public

   private
    Femail_40: String;
    FnmCtt_36: String;
    FcpfCtt_37: String;
    FfoneCel_39: String;
    FfoneFixo_38: String;

   property nmCtt_36: String read FnmCtt_36 write FnmCtt_36;
   property cpfCtt_37: String read FcpfCtt_37 write FcpfCtt_37;
   property foneFixo_38: String read FfoneFixo_38 write FfoneFixo_38;
   property foneCel_39: String read FfoneCel_39 write FfoneCel_39;
   property email_40: String read Femail_40 write Femail_40;
       function gerarTX2(): string;

 end;
implementation

{ Tcontato }

function Tcontato.gerarTX2: string;
var
  Lista: TStringList;
begin

  Lista := TStringList.Create;
  Lista.Add('ideMinLei_27=' + ideMinLei_27);
  Lista.Add('nrCertif_28=' + nrCertif_28);
  Lista.Add('dtEmisCertif_29=' + dtEmisCertif_29);
  Lista.Add('dtVencCertif_30=' + dtVencCertif_30);

  if (Trim(nrProtRenov_31) <> EmptyStr) then
    Lista.Add('nrProtRenov_31=' + nrProtRenov_31);

  if (Trim(dtProtRenov_32) <> EmptyStr) then
    Lista.Add('dtProtRenov_32=' + dtProtRenov_32);

  if (Trim(dtDou_33) <> EmptyStr) then
    Lista.Add('dtDou_33=' + dtDou_33);

  if (Trim(pagDou_34) <> EmptyStr) then
    Lista.Add('pagDou_34=' + pagDou_34);

  Result := Lista.Text;
end;


end.
